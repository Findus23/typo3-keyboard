{
  "manifest_version": 2,
  "name": "TYPO3 Keyboard Control",
  "short_name": "TYPO3 Keyboard Control",
  "version": "1.0.1",
  "description": "Produktives Arbeiten in TYPO3 mithilfe von Keyboard Shortcuts",
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/typo3/*"
      ],
      "all_frames": true,
      "js": [
        "libs/mousetrap.js",
        "contentscript.js"
      ]
    }
  ],
  "applications": {
    "gecko": {
      "id": "{9a7acde7-4af3-47d9-8061-c60811b351f2}",
      "strict_min_version": "42.0"
    }
  }
}
